{
  "name": "phaser-es6-boilerplate",
  "version": "1.0.2",
  "description": "Tiny flexible template for writing Phaser games in ES6/ES2015 with Babel.",
  "main": "src/index.js",
  "scripts": {
    "clear": "node ./node_modules/rimraf/bin.js ./dist",
    "compile": "node ./node_modules/webpack/bin/webpack --bail --progress --profile",
    "build": "npm run clear && npm run compile",
    "server": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server --hot --history-api-fallback --inline --progress",
    "start": "npm run build && npm run server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/belohlavek/phaser-es6-boilerplate"
  },
  "author": "Daniel Belohlavek",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "expose-loader": "^0.7.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "mime": "^1.3.6",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "script-loader": "^0.7.0",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "phaser": "^2.4.7"
  }
}
